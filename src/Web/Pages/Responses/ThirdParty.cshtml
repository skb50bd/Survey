@page "{id:int}"
@model Web.Pages.Responses.ThirdPartyModel
@{
    ViewData["Title"] = "Third Party Representative Response";
}

<div class="card">
    <div class="card-body pb-0">
        <h1 class="card-title display-4">
            @ViewData["Title"]
        </h1>

        <div style="font-size: x-large; font-weight: 400;"
             class="mb-2 text-right card-subtitle">
            @Model.Tpr.Name
            <<email>@Model.Tpr.Email</email>>
        </div>

        <table class="table table-striped table-bordered table-hover">
            <thead class="thead-dark">
                <tr>
                    <th class="w-75">
                        Question
                    </th>

                    <th class="w-25 text-right">
                        Response
                    </th>
                </tr>
            </thead>

            <!--DOCUMENTS-->
            <tbody>
                <tr>
                    <th colspan="2" class="text-right">
                        FILE DOWNLOAD
                    </th>
                </tr>

                <tr>
                    <td colspan="2">
                        @{ var disabled = Model.TprResponse.DocumentAId is null ? "disabled" : ""; }
                        <a class="btn btn-sm btn-block btn-link btn-secondary text-white @disabled"
                           asp-controller="Files"
                           asp-action="Get"
                           asp-route-id="@Model.TprResponse.DocumentAId">
                            (A) Certificate
                        </a>

                        @{ disabled = Model.TprResponse.DocumentBId is null ? "disabled" : ""; }
                        <a class="btn btn-sm btn-block btn-link btn-secondary text-white @disabled"
                           asp-controller="Files"
                           asp-action="Get"
                           asp-route-id="@Model.TprResponse.DocumentBId">
                            (B) Business Brochures / Annual Report
                        </a>

                        @{ disabled = Model.TprResponse.DocumentCId is null ? "disabled" : ""; }
                        <a class="btn btn-sm btn-block btn-link btn-secondary text-white @disabled"
                           asp-controller="Files"
                           asp-action="Get"
                           asp-route-id="@Model.TprResponse.DocumentCId">
                            (C) Financial Statements
                        </a>

                        @{ disabled = Model.TprResponse.DocumentDId is null ? "disabled" : ""; }
                        <a class="btn btn-sm btn-block btn-link btn-secondary text-white @disabled"
                           asp-controller="Files"
                           asp-action="Get"
                           asp-route-id="@Model.TprResponse.DocumentDId">
                            (D) CV
                        </a>

                        @{ disabled = Model.TprResponse.DocumentEId is null ? "disabled" : ""; }
                        <a class="btn btn-sm btn-block btn-link btn-secondary text-white @disabled"
                           asp-controller="Files"
                           asp-action="Get"
                           asp-route-id="@Model.TprResponse.DocumentEId">
                            (E) Policies
                        </a>
                    </td>
                </tr>
            </tbody>

            <!--FORM COMPLETION INFORMATION-->
            <tbody>
                <tr>
                    <th colspan="2" class="text-right">
                        FORM COMPLETION INFORMATION
                    </th>
                </tr>

                <tr>
                    <td>
                        Date
                    </td>
                    <td class="text-right">
                        @Model.TprResponse.A1
                    </td>
                </tr>

                <tr>
                    <td>
                        a. Name
                    </td>
                    <td class="text-right">
                        @Model.TprResponse.A2A
                    </td>
                </tr>

                <tr>
                    <td>
                        b. E-Mail
                    </td>
                    <td class="text-right">
                        <email>@Model.TprResponse.A2B</email>
                    </td>
                </tr>

                <tr>
                    <td colspan="2">
                        c. Telephone
                    </td>
                </tr>

                <tr>
                    <td>
                        c.1. Landline
                    </td>
                    <td class="text-right">
                        <tel>@Model.TprResponse.A2C1</tel>
                    </td>
                </tr>

                <tr>
                    <td>
                        c.2. Cell
                    </td>
                    <td class="text-right">
                        <tel>@Model.TprResponse.A2C2</tel>
                    </td>
                </tr>
            </tbody>

            <!--BUSINESS INFORMATION-->
            <tbody>
                <tr>
                    <th colspan="2" class="text-right">
                        BUSINESS INFORMATION
                    </th>
                </tr>

                <tr>
                    <td>
                        1. Your organization’s Full
                        Legal Name, hereafter to be referred to as COMPANY
                    </td>
                    <td class="text-right">
                        @Model.TprResponse.B1
                    </td>
                </tr>

                <tr>
                    <td colspan="2">
                        2. COMPANY Information
                    </td>
                </tr>

                <tr>
                    <td>
                        a. Tax number/Jurisdiction
                    </td>
                    <td class="text-right">
                        @Model.TprResponse.B2A
                    </td>
                </tr>

                <tr>
                    <td>
                        b. Main office address
                    </td>
                    <td class="text-right">
                        @Model.TprResponse.B2B
                    </td>
                </tr>

                <tr>
                    <td>
                        c. Address of any other location
                        in the country from which COMPANY
                        will be providing services.
                        If none, please indicate.
                    </td>
                    <td class="text-right">
                        @Model.TprResponse.B2C
                    </td>
                </tr>

                <tr>
                    <td colspan="2">
                        d. Telephone
                    </td>
                </tr>

                <tr>
                    <td>
                        d.1. Landline
                    </td>
                    <td class="text-right">
                        <tel>@Model.TprResponse.B2D1</tel>
                    </td>
                </tr>

                <tr>
                    <td>
                        d.2. Cell
                    </td>
                    <td class="text-right">
                        <tel>@Model.TprResponse.B2D2</tel>
                    </td>
                </tr>

                <tr>
                    <td>
                        e. Email
                    </td>
                    <td class="text-right">
                        <email>@Model.TprResponse.B2E</email>
                    </td>
                </tr>

                <tr>
                    <td>
                        f. Website
                    </td>
                    <td class="text-right">
                        <url>@Model.TprResponse.B2F</url>
                    </td>
                </tr>

                <tr>
                    <td style="font-style: italic;" colspan="2">
                        Company Information if it is am Individual
                    </td>
                </tr>

                <tr>
                    <td>
                        a. Personal Name
                    </td>
                    <td class="text-right">
                        @Model.TprResponse.B2Oa
                    </td>
                </tr>

                <tr>
                    <td>
                        b. Date of Birth
                    </td>
                    <td class="text-right">
                        @Model.TprResponse.B2Ob
                    </td>
                </tr>

                <tr>
                    <td>
                        c. ID number
                    </td>
                    <td class="text-right">
                        @Model.TprResponse.B2Oc
                    </td>
                </tr>

                <tr>
                    <td>
                        d. Tax number/Juristriction
                    </td>
                    <td class="text-right">
                        @Model.TprResponse.B2Od
                    </td>
                </tr>

                <tr>
                    <td colspan="2" style="font-style:italic;">
                        Organizational information relating to COMPANY
                    </td>
                </tr>

                <tr>
                    <td>
                        a. COMPANY business structure
                    </td>
                    <td class="text-right">
                        @Model.TprResponse.B3A
                    </td>
                </tr>

                <tr>
                    <td colspan="2">
                        b. Approximate establishment
